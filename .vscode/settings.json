{
  "workbench.sideBar.location": "right",
  "window.zoomLevel": 0,
  "editor.formatOnSave": true,
  "workbench.colorTheme": "Electron",
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "expand",
  "editor.suggest.filterGraceful": false,
  "workbench.iconTheme": "material-icon-theme",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "prettier.singleQuote": true,
  "html.format.wrapLineLength": 80,
  "editor.largeFileOptimizations": false,
  "explorer.confirmDragAndDrop": false,
  "javascript.preferences.quoteStyle": "single",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.minimap.maxColumn": 80,
  "html.format.endWithNewline": true,
  "html.format.indentInnerHtml": true,
  "html.customData": [".html"],
  "[django-html]": {},
  "terminal.integrated.inheritEnv": false,
  "C_Cpp.updateChannel": "Insiders",
  "editor.wrappingIndent": "indent",
  "editor.wordWrap": "on",
  "cSpell.enableFiletypes": ["django-html", "shellscript", "sql", "sqlite"],
  "[python]": {
    "editor.defaultFormatter": "autopep8",
    "editor.minimap.maxColumn": 80
  },
  "prettier.bracketSpacing": true,
  "git.suggestSmartCommit": false,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontSize": 15,
  "editor.multiCursorModifier": "alt",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.statusBar.visible": true,
  "editor.renderWhitespace": "all",
  "files.autoSave": "afterDelay",
  "editor.wordWrapColumn": 100,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.format.enable": true,
  "files.exclude": {
    "**/.git": false,
    "**/*.git": true
  },
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "cSpell.language": "en,en-US,en-GB",
  "http.proxySupport": "off",
  "cSpell.userWords": [
    "AUTOINCREMENT",
    "Junit",
    "bcalm",
    "blpop",
    "brpop",
    "fizzbuzz",
    "hgetall",
    "hmset",
    "lrange",
    "mildshower",
    "mixins",
    "pokemons",
    "rahit",
    "rahulmadaan",
    "ramu",
    "recallability",
    "rpoplpush",
    "rpush",
    "splitted",
    "trinangkur",
    "unfollow"
  ],
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.home": "/Library/Java/JavaVirtualMachines/jdk-14.jdk/Contents/Home/",
  "java.semanticHighlighting.enabled": true,
  "extensions.ignoreRecommendations": true,
  "java.configuration.checkProjectSettingsExclusions": false
}
